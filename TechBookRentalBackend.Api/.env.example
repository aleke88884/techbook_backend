# Application Settings
ASPNETCORE_ENVIRONMENT=Development

# Database
DATABASE_CONNECTION_STRING=Host=localhost;Database=techbook_db;Username=postgres;Password=your_password

# JWT Settings (CHANGE THIS IN PRODUCTION!)
JWT_SECRET=YourSuperSecretKeyThatIsAtLeast32CharactersLong!

# API Keys (for future geocoding providers)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
MAPBOX_API_KEY=your_mapbox_api_key

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Rate Limiting
RATE_LIMIT_PERMITS_PER_MINUTE=60

# Logging
LOG_LEVEL=Information
SEQ_URL=http://localhost:5341

# Geocoding Settings (Kazakhstan)
GEOCODING_COUNTRY_CODE=kz
GEOCODING_LANGUAGE=ru
GEOCODING_MAX_RESULTS=5
