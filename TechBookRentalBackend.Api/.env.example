# Application Settings
ASPNETCORE_ENVIRONMENT=Development

# Database (for future use)
DATABASE_CONNECTION_STRING=Host=localhost;Database=techbook_rental;Username=postgres;Password=your_password

# API Keys (for future geocoding providers)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
MAPBOX_API_KEY=your_mapbox_api_key

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Rate Limiting
RATE_LIMIT_PERMITS_PER_MINUTE=60

# Logging
LOG_LEVEL=Information
SEQ_URL=http://localhost:5341

# Geocoding Settings (Kazakhstan)
# Country code for restricting geocoding results
GEOCODING_COUNTRY_CODE=kz
# Language for results: ru (Russian), kk (Kazakh), en (English)
GEOCODING_LANGUAGE=ru
# Maximum number of address suggestions
GEOCODING_MAX_RESULTS=5
